{
  "name": "wild-orchid-watch-pwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:debug": "BROWSERSLIST_ENV=debug vue-cli-service serve",
    "serve:safari": "BROWSERSLIST_ENV=safari vue-cli-service serve",
    "build": "vue-cli-service build && rollup -c && workbox injectManifest",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "build:serve": "BROWSERSLIST_ENV=debug DISABLE_MINIFY=1 yarn build && servor dist/ index.html --no-browser",
    "build:serve:safari": "BROWSERSLIST_ENV=safari yarn build && servor dist/ index.html --no-browser",
    "build:serve:watch": "nodemon --ext vue,js,mjs,json --watch src --watch sw-src --watch package.json --exec bash -c 'yarn build:serve'",
    "firebase:deploy:ci": "doit() { firebase --non-interactive --token ${FIREBASE_TOKEN:?} deploy --only hosting:${1:?}; }; doit",
    "prettier:check": "prettier \"**/*.{vue,scss,sass,js}\" --list-different",
    "prettier:format-all": "prettier --write \"**/*.{vue,scss,sass,js}\"",
    "test:e2e:headless": "vue-cli-service test:e2e --headless",
    "test:unit:debug": "node --inspect-brk ./node_modules/.bin/vue-cli-service test:unit --runInBand"
  },
  "dependencies": {
    "@sentry/browser": "^5.6.2",
    "@sentry/integrations": "^5.6.1",
    "base64-js": "^1.3.1",
    "comlink": "4.2.0",
    "core-js": "3.6.4",
    "dms2dec": "^1.1.0",
    "exif-js": "2.3.0",
    "exif-reader": "^1.0.3",
    "fetch-retry": "^2.2.3",
    "formdata-polyfill": "^3.0.19",
    "is_js": "^0.9.0",
    "jimp": "^0.9.3",
    "jsonwebtoken": "^8.5.1",
    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "ml5": "0.4.3",
    "moment": "^2.24.0",
    "onsenui": "^2.10.9",
    "piexifjs": "^1.0.6",
    "pkce-challenge": "^2",
    "pwacompat": "^2.0.9",
    "register-service-worker": "^1.6.2",
    "sass": "^1.21.0",
    "smoothscroll-polyfill": "^0.4.4",
    "uuid": "^3.3.3",
    "vue": "^2.6.10",
    "vue-analytics": "^5.17.2",
    "vue-head": "^2.1.1",
    "vue-onsenui": "^2.6.2",
    "vue-router": "3.0.7",
    "vue2-google-maps": "0.10.7",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.5.4",
    "worker-plugin": "3.2.0"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.8.0",
    "@types/lodash": "^4.14.132",
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-e2e-cypress": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-plugin-pwa": "^4.1.1",
    "@vue/cli-plugin-unit-jest": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.2",
    "fake-indexeddb": "^3.0.0",
    "favicons-webpack-plugin": "^1.0.1",
    "firebase-tools": "^6.10.0",
    "nodemon": "^2.0.0",
    "promise-worker": "2.0.1",
    "rollup": "^1.27.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sass-loader": "^8.0.0",
    "servor": "^2.2.1",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-merge": "^4.2.1",
    "workbox-background-sync": "^4.3.1",
    "workbox-cli": "^4.3.1",
    "workbox-routing": "^4.3.1",
    "workbox-strategies": "^4.3.1",
    "worker-loader": "2.0.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": {
    "production": [
      "ChromeAndroid >= 42",
      "FirefoxAndroid >= 39",
      "iOS >= 10.3"
    ],
    "debug": [
      "last 2 Chrome versions"
    ],
    "safari": [
      "iOS >= 10",
      "Safari >= 10"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "mjs"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.(jsx?|mjs)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "setupFiles": [
      "fake-indexeddb/auto"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/unit/setup.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!workbox-).+\\.js$"
    ]
  },
  "_id": "wild-orchid-watch-pwa@0.1.0",
  "engines": {
    "npm": ">=5.5.0",
    "node": ">=8.16.0"
  },
  "readme": "ERROR: No README data found!"
}
